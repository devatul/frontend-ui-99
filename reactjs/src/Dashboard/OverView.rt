<rt-require dependency="react-router/lib/Link" as="Link"/>
<section>
    
    <!--bar_menu start-->
    <div class="my_main bar_menu container">
            <div class="row">
                <div class="col-md-12 mb-md">
                    <div class="row">
                        <div class="col-sm-8">
                            <p class="page-heading-sm">Dashboard Overview</p>
                            <div class="filter-tags-block">
                                <label class="pull-left mr-md" style="display:none">Applied Filters:</label>
                                <div class="pull-left filter-tags">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4">
                            <ul class="filter-buttons pull-right">
                                <li class="dropdown">
                                    <a class="toggle-button btn btn-default" onClick={this.filterScan}><i class="fa fa-filter" aria-hidden="true"></i></a>
                                    <div class="dropdown-backdrop-custom" style="display:none"></div>
                                    <div class="dropdown-menu has-child has-arrow dd-md dd-right dd-filter">
                                        <ul class="filter-list list-unstyled pt-xs">
                                            <li class="mb-sm">
                                                <div class="form-group">
                                                    <select id="category" class="form-control select-multiple overview-filter" multiple="multiple" data-title="Category">
                                                        <option rt-repeat="category in this.state.label.list_category" key="{category.id}" value="{category.name}">{category.name}</option>
                                                    </select>
                                                </div>
                                            </li>
                                            <li class="mb-sm">
                                                <div class="form-group">
                                                    <select id="confidentiality" class="form-control select-multiple overview-filter" multiple="multiple" data-title="Confidentiality">
                                                        <option rt-repeat="confidentiality in this.state.label.list_confidentiality" key="{confidentiality.id}" value="{confidentiality.name}">{confidentiality.name}</option>
                                                    </select>
                                                </div>
                                            </li>
                                            <li class="mb-sm">
                                                <div class="form-group">
                                                    <select id="doctype" class="form-control select-multiple overview-filter" multiple="multiple" data-title="Document Type">
                                                        <option rt-repeat="doctype in this.state.label.list_doctype" key="{doctype.id}" value="{doctype.name}">{doctype.name}</option>
                                                    </select>
                                                </div>
                                            </li>
                                            <li class="mb-sm">
                                                <div class="form-group">
                                                    <select id="language" class="form-control select-multiple" multiple="multiple" data-title="Language">
                                                        <option rt-repeat="language in this.state.label.list_language" key="{language.id}" value="{language.name}">{language.name}</option>
                                                    </select>
                                                </div>
                                            </li>
                                        </ul>
                                    </div>
                                </li>
                                <li class="dropdown">
                                    <a data-toggle="dropdown" class="toggle-button btn btn-default"><i class="fa fa-info-circle" aria-hidden="true"></i></a>
                                    <div class="dropdown-menu has-arrow dd-md full-mobile dd-right">
                                        <div class="table-responsive">
                                            <table class="table general-table mb-none">
                                                <tbody>
                                                <tr>
                                                    <td>Last Data Scan:</td>
                                                    <td colspan="3">{this.state.scan_result.last_data_scan}</td>
                                                </tr>
                                                <tr>
                                                    <td>Country In Scope:</td>
                                                    <td colspan="3">{this.state.scan_result.country}</td>
                                                </tr>
                                                <tr>
                                                    <td>Business In Scope:</td>
                                                    <td colspan="3">{this.state.scan_result.business_unit}</td>
                                                </tr>
                                                <tr>
                                                    <td>Scan Status:</td>
                                                    <td colspan="3">{this.state.scan_result.scan_status}</td>
                                                </tr>
                                                <tr>
                                                    <td>Documents Analyzed:</td>
                                                    <td colspan="3">{this.state.scan_result.document_analyzed}</td>
                                                </tr>
                                                <tr>
                                                <td>File Extension Processed:</td>
                                                    <td colspan="3">{this.state.scan_result.file_extensions_processed}</td>
                                                </tr>
                                                <tr>
                                                    <td>Duplicated Documents:</td>
                                                    <td colspan="3">{this.state.scan_result.total_duplicates}</td>
                                                </tr>
                                                <tr>
                                                    <td>Encrypted Documents or Archives:</td>
                                                    <td colspan="3">{this.state.scan_result.encrypted_documents}</td>
                                                </tr>
                                                <tr>
                                                    <td>Documents Skipped:</td>
                                                    <td colspan="3">{this.state.scan_result.documents_skipped}</td>
                                                </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>

